!function(n){var t,o=n("[data-general-info-description]"),e=o.find(".pp-description_title"),s=e.text();if(s.length>675){var i=s.indexOf(" ",675);-1!==i&&function(){t=s.substr(0,i);var r=n('<span class="button-more font-underline" role="button">more</span>'),l=n('<span class="button-less font-underline" role="button">less</span>');function u(){e.text(t).append(r)}u(),e.on("click",".button-more",(function(){e.text(s).append(l)})),e.on("click",".button-less",(function(){u(),n(document).trigger("PREVENT_SCROLL_DOWN"),n("html, body").scrollTop(o.offset().top-350)}))}()}}(jQuery);