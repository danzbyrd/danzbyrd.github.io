<section class="mv_email_form full_width">
    <h3 class="email_heading">Awaken your unstoppable. Subscribe for daily insights on personal growth.</h3>
    <form id="emailDailyForm">
        <div class="daily-form-container">
            <input type="text" id="name" name="name" class="name_form_input form_input_field" placeholder="Your Name">
            <input type="email" class="email_form_input form_input_field" placeholder="Email address" name="email" required>
            <input type="submit" class="email_submit_btn" value="Subscribe">
        </div>
    </form>
</section>

<script>
    document.addEventListener("DOMContentLoaded", function(e) {
        const url = "https://platform-api.mindvalley.com/api/leads/emails";
        const emailDailyForm = document.getElementById('emailDailyForm');
        const emailDailyFormContainer = document.querySelector('.mv_email_form');
        const submitButton = document.querySelector("#emailDailyForm input.email_submit_btn");
        const inputForm_email = document.querySelector("#emailDailyForm input.email_form_input");
        const inputForm_name = document.querySelector("#emailDailyForm input.name_form_input");

        const checkCookie = function() {
            var pageCookies = document.cookie.toString().split(';');
            if (pageCookies.find(v => v.includes("mv_signup_form_registration_email"))) {
                emailDailyFormContainer.remove();
                console.log("User has already subscribed to the newsletter");
            } else {
                console.log("No Cookie Found");
            }
        }

        // checkCookie();

        const setSignupCookie = function(email, monthsToExp = 6) {
            let cookieExpDate = new Date();
            cookieExpDate.setMonth(cookieExpDate.getMonth() + monthsToExp);
            console.log("cookieAdded", `mv_signup_form_registration_email=${email}; expires=${cookieExpDate.toUTCString()}`);
            document.cookie = `mv_signup_form_registration_email=${email}; expires=${cookieExpDate.toUTCString()}`;
        }

        const removeSignupCookie = function(monthsToExp = 6) {
            let cookieExpDate = new Date();
            cookieExpDate.setMonth(cookieExpDate.getMonth() - monthsToExp);
            console.log("cookieRemoved");
            document.cookie = `mv_signup_form_registration_email=; expires=${cookieExpDate.toUTCString()}`;
        }

        const toggleLoading = function(loading = false) {
            console.log(`Toggling state to`, loading)
            if (loading) {
                submitButton.value = "Please wait...";
                submitButton.style.opacity = .5;
                submitButton.style.cursor = "not-allowed";
                submitButton.style.pointerEvents = "none";
                submitButton.disabled = true;
                inputForm_email.disabled = true;
                inputForm_name.disabled = true;
                inputForm_email.style.opacity = .5;
                inputForm_name.style.opacity = .5;
            } else {
                submitButton.value = "Sign Up Now";
                submitButton.style.opacity = 1;
                submitButton.style.cursor = "pointer";
                submitButton.style.pointerEvents = "initial";
                submitButton.disabled = false;
                inputForm_email.disabled = false;
                inputForm_name.disabled = false;
                inputForm_email.style.opacity = 1;
                inputForm_name.style.opacity = 1;
            }
        }

        var formData = new FormData(emailDailyForm);
        emailDailyForm.onsubmit = function(event) {
            event.preventDefault();

            if (!inputForm_email.value || inputForm_email.value.length <= 0) {
                alert("Email address cannot be blank");
                toggleLoading(false);
                return;
            }

            if (!inputForm_name.value || inputForm_name.value.length <= 0) {
                alert("Full name cannot be blank");
                toggleLoading(false);
                return;
            }

            toggleLoading(true);
            let nameSplits = inputForm_name.value.split(" ");
            let fName = "";
            let lName = "";

            if (nameSplits.length === 0) {
                fName = inputForm_name;
            } else if (nameSplits.length === 1) {
                fName = nameSplits[0];
            } else if (nameSplits.length > 2) {
                fName = nameSplits.slice(0, -1).join(' ');
                lName = nameSplits.slice(-1).join(' ');
            } else {
                fName = nameSplits[0];
                lName = nameSplits[1];
            }

            var dataToSend = {
                contact: {
                    email: inputForm_email.value,
                    first_name: fName,
                    last_name: lName,
                    add_tags: [
                        "blog_homepage_daily"
                    ]
                },
                auth_token: "b94b5bac35ed766087f28a844b8e2e40abd531f0"
            }
            debugger;

            jQuery.ajax({
                    method: "POST",
                    url: url,
                    headers: {
                        'Content-Type': 'application/json',
                        'Accept': 'application/json'
                    },
                    data: JSON.stringify(dataToSend)
                })
                .done(function(msg) {
                    // setSignupCookie(inputForm_email.value);
                    triggerSuccess();
                    return false;
                });
            return false;
        }

        const triggerSuccess = function() {
            alert("You're all set. Welcome to Mindvalley!");
            toggleLoading(false);
            // checkCookie();
        }

    })
</script>

